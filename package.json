{
  "name": "eleventy-ai-site",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:css": "npx tailwindcss -i ./input.css -o ./_site/css/style.css --minify",
    "build:eleventy": "eleventy",
    "build": "npm-run-all build:eleventy build:css",
    "start:css": "npx tailwindcss -i ./input.css -o ./_site/css/style.css --watch",
    "start:eleventy": "eleventy --serve",
    "start": "npm-run-all --parallel start:eleventy start:css"
  },
